{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "display": {
    "separator": "➜   "
  },
  "logo": {
    "source": "~/.dotfiles/modules/fastfetch/logo.txt",
    "type": "file",
    "color": {
                "1": "{{colors.primary.default.hex}}",
                "2": "{{colors.tertiary.default.hex}}",
                "3": "{{colors.primary_fixed_dim.default.hex}}"
            }
  },
  "modules": [
    "break",
    {
      "format": "                             {6}{7}{8}",
      "type": "title"
    },
    "break",
    {
      "format": "┌────────────────────────────────────────────────────────────────────┐",
      "type": "custom"
    },
    "break",
    {
      "key": "  OS",
      "keyColor": "{{colors.primary.default.hex}}",
      "outputColor": "{{colors.tertiary.default.hex}}",
      "type": "os"
    },
    {
      "key": "  Machine",
      "keyColor": "{{colors.on_surface_variant.default.hex}}",
      "outputColor": "{{colors.tertiary.default.hex}}",
      "type": "host"
    },
    {
      "key": "  Kernel",
      "keyColor": "{{colors.tertiary.default.hex}}",
      "outputColor": "{{colors.tertiary.default.hex}}",
      "type": "kernel"
    },
    {
      "key": "  Uptime",
      "keyColor": "{{colors.primary_fixed_dim.default.hex}}",
      "outputColor": "{{colors.tertiary.default.hex}}",
      "type": "uptime"
    },
    {
      "key": "  Packages",
      "keyColor": "{{colors.tertiary.default.hex}}",
      "outputColor": "{{colors.tertiary.default.hex}}",
      "type": "packages"
    },
    {
      "key": "  WM",
      "keyColor": "{{colors.primary.default.hex}}",
      "outputColor": "{{colors.tertiary.default.hex}}",
      "type": "wm"
    },
    {
      "key": "  Shell",
      "keyColor": "{{colors.primary.default.hex}}",
      "outputColor": "{{colors.tertiary.default.hex}}",
      "type": "shell"
    },
    {
      "key": "  Terminal",
      "keyColor": "{{colors.tertiary.default.hex}}",
      "outputColor": "{{colors.tertiary.default.hex}}",
      "type": "terminal"
    },
    {
      "key": "  Font",
      "keyColor": "{{colors.primary_fixed_dim.default.hex}}",
      "outputColor": "{{colors.tertiary.default.hex}}",
      "type": "terminalfont"
    },
    {
      "key": "  CPU",
      "keyColor": "{{colors.primary.default.hex}}",
      "outputColor": "{{colors.tertiary.default.hex}}",
      "type": "cpu"
    },
    {
      "key": "  GPU",
      "keyColor": "{{colors.tertiary.default.hex}}",
      "outputColor": "{{colors.tertiary.default.hex}}",
      "type": "gpu"
    },
    {
      "key": "  Wifi",
      "keyColor": "{{colors.primary_fixed_dim.default.hex}}",
      "outputColor": "{{colors.tertiary.default.hex}}",
      "type": "wifi"
    },
    {
      "compact": true,
      "key": "  Local IP",
      "keyColor": "{{colors.tertiary.default.hex}}",
      "outputColor": "{{colors.tertiary.default.hex}}",
      "type": "localip"
    },
    "break",
    {
      "format": "└────────────────────────────────────────────────────────────────────┘",
      "type": "custom"
    },
    "break",
    {
      "block": {
        "width": 10
      },
      "paddingLeft": 34,
      "symbol": "circle",
      "type": "colors"
    }
  ]
}
